# Use Keeling_Data again,
# compute the annual mean of CO2 since 1959,
# plot your results.
year <- seq(1959, 2020, by=1)
annual_co2 <- c()
for(i in year){
list <- which(Keeling_Data$year == i)
annual_co2 <- c(annual_co2,
mean( Keeling_Data$co2[list], na.rm=T))
}
plot(year, annual_co2, type="l", lwd=5, col="blue",
xlab="Year", ylab="Annual CO2 [ppm]")
# Monthly CO2
month <- seq(1, 12, by=1)
month_labels <- c("January", "February", "March",
"April","May", "June",
"July", "August","September",
"October","November","December")
monthly_co2 <- c()
for(i in month){
list <- which(Keeling_Data$month == month_labels[i])
monthly_co2 <- c(monthly_co2,
mean( Keeling_Data$co2[list], na.rm=T))
}
plot(month, monthly_co2, type="l", lwd=5, col="blue",
xlab="Year", ylab="Monthly CO2 [ppm]")
